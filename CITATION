citHeader("To cite nadda in publications use:")

citEntry(entry = "Article",
  title        = "A Fast Alignment-Free Approach for De Novo Detection of Protein Conserved Regions",
  author       = personList(as.person("Armen Abnousi"),
                   as.person("Shira L Broschat"),
                   as.person("Ananth Kalyanaraman")),
  journal      = "PloS one",
  year         = "2016",
  volume       = "11",
  number       = "8",
  pages        = "e0161338",

  textVersion  =
  paste("Armen Abnousi, Shira L Broschat, Ananth Kalyanaraman (2016).",
        "A Fast Alignment-Free Approach for De Novo Detection of Protein Conserved Regions.",
        "PloS one, 11(8), e0161338",
)

